{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jquery": true,
        "node": true
    },
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "classes": true,
        "jsx": true
      }
    },
    "globals": {
        "FB": true,
        "angular": true,
        "_": true,
        "PrettyJSON": true
    },
    "extends": "google",
    "rules": {
        "max-len": [1, 500, 4, {
          "ignoreComments": true,
          "ignoreUrls": true
        }],
        "valid-jsdoc": "off",
        "require-jsdoc": "off",
        "no-warning-comments": "off",
        "no-multi-spaces": "off",
        "no-else-return": "off",
        "key-spacing": "off",
        "quote-props": "off",
        "guard-for-in": "off",
        "no-negated-condition": "off",
        "no-unused-vars": "off"
    }
}
